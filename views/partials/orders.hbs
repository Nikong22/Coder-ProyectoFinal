<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Num orden</th>
            <th scope="col">Fecha</th>
            <th scope="col">Estado</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
    {{#each orders}}
        <tr>
            <td>{{this.numOrder}}</td>
            <td>{{this.fecha}}</td>
            <td>{{this.estado}}</td>
            <td>
                <a href="/api/order/{{this._id}}">Ver orden</a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
